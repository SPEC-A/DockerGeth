

 ether.Node.com:
  image: 'ethereum'
  tty: true
  ports: 
   - "httpport:httpport" 
   - "rpcport:rpcport"
  environment:
   ENV : ETHERNODE1
   HTTPPORT: httpport
   PORT : rpcport
  networks:
   eth-net:
    ipv4_address : IP
  container_name : ether.Node.com
  command : /bin/sh -c "/home/data/geth.sh"
  working_dir : /home/DATA_STORE
  depends_on:
   - ether.log-server.com
   - ether.bootnode.com
